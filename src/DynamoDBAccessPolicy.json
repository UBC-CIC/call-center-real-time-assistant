{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "dynamodb:BatchGet*",
        "dynamodb:PutItem",
        "dynamodb:DescribeTable",
        "dynamodb:Get*",
        "dynamodb:BatchWrite*",
        "dynamodb:Scan",
        "dynamodb:Query",
        "dynamodb:DescribeStream"
      ],
      "Resource": "arn:aws:dynamodb:us-west-2:*:table/*"
    }
  ]
}